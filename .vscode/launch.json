    "version": "0.2.0",
    "python": "${command:python.interpreterPath}",
    "configurations": [

        {
            "name": "defectdojo-importer",
            "type": "debugpy",
            "request": "launch",
            "module": "poetry",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": "run defectdojo-importer ${command:pickArgs}",
            "envFile": "${workspaceFolder}/.env",
            "showReturnValue": true,
            "redirectOutput": true,
        }
    ]
}